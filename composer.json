    "require": {
        "php": "^8",
        "facile-it/php-codec": "^0.0.2"
    },
    "require-dev": {
        "facile-it/facile-coding-standard": "^0.5.1",
        "phpstan/phpstan": "^1.2",
        "phpunit/phpunit": "^9.5",
        "vimeo/psalm": "^4.13"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Jean85\\ValueObjectPlayground\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Jean85\\ValueObjectPlayground\\": "tests/"
        }
    }
